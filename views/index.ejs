<script>
    var OL_Action_Root = "http://127.0.0.1:3000/index";
    var xmlHttp = null;
    function Req_ajax(info, type)
    {
        $.ajax({
            data: {info:info, type:type},
            url: OL_Action_Root+"/showSection/1",
            dataType: 'json',
            cache: false,
            timeout: 5000,
            type:type,    // 如果要使用GET方式，则将此处改为'get'
            success: function(data){
                var res = data;
                console.log(data);
                if(res.message == 'success')
                {
                    document.getElementById("status").innerHTML += "<p style='color:green; font-weight:bold;'>服务器返回信息: " + res.data + "</p>";
                }
                else
                {
                    document.getElementById("status").innerHTML += "<p style='color:#2289DB; font-weight:bold;'>获取服务器信息失败！ " + res.data + "</p>";
                }
            },
            error: function(jqXHR, textStatus, errorThrown){
                document.getElementById("texta").innerHTML=errorThrown;


                document.getElementById("status").innerHTML += "<p style='color:#C00000; font-weight:bold;'>连接不到服务器，请检查网络！</p>";
            }
        });
    }
</script>


<script>
    function setSection(event) {
        // 6 create an instance when the DOM is ready
//        $('#jstree').jstree();
        alert("执行了！");

        $("#jstree").jstree({
            "core" : {
                "themes" : {
                    "responsive": false
                },
                // so that create works
                "check_callback" : true,
                'url': "http://127.0.0.1/3000/index/ajax"+"/req_ajax",
                'data' : function (obj, callback) {
                    var jsonstr="[]";
                    var jsonarray = eval('('+jsonstr+')');
                    $.ajax({
                        data: {info:'get请求', type:'get'},
                        url: "http://127.0.0.1/3000/index/ajax/"+"/req_ajax",
                        dataType: 'json',
                        cache: false,
                        timeout: 5000,
                        type:'get',    // 如果要使用GET方式，则将此处改为'get'
                        success: function(data){
                            var res = data;
                            if(res[0] == 'success')
                            {
                                document.getElementById("status").innerHTML += "<p style='color:green; font-weight:bold;'>服务器返回信息: " + res[1] + "</p>";
                            }
                            else
                            {
                                document.getElementById("status").innerHTML += "<p style='color:#2289DB; font-weight:bold;'>获取服务器信息失败！ " + res[1] + "</p>";
                            }
                        },
                        error: function(jqXHR, textStatus, errorThrown){
                            document.getElementById("texta").innerHTML=errorThrown;


                            document.getElementById("status").innerHTML += "<p style='color:#C00000; font-weight:bold;'>连接不到服务器，请检查网络！</p>";
                        }
                    });
                    callback.call(this, jsonarray);
                }
            },
            "types" : {
                "default" : {
                    "icon" : "glyphicon glyphicon-flash"
                },
                "file" : {
                    "icon" : "glyphicon glyphicon-ok"
                }
            },
            "state" : { "key" : "demo2" },
            "plugins" : [ "dnd", "state", "types","checkbox","wholerow" ]
        });

        // 7 bind to events triggered on the tree
        $('#jstree').on("changed.jstree", function (e, data) {
            alert(data.instance.get_node(data.selected).text);      //输出当前选中的区域的名称
        });
    }
</script>



